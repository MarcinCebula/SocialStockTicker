div class="navbar navbar-fixed-top"
  div class="navbar-inner"
    div class="container"
      a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"
        span class="icon-bar"
        span class="icon-bar"
        span class="icon-bar"

			a class="brand" href="#{root_path}" alt="Find My Next Apartment Logo" SocialStockTicker 
			div class="nav-collapse"
				ul class="nav"
					- if request.path == root_path
						li  class="active"
							a class="test_class" href="#{root_path}" Home
					- else
						li  class=""
							a class="test_class" href="#{root_path}" Home
					- if request.path == docs_path
						li  class="active"
							a href="#{docs_path}" Docs
					- else
						li  class=""
							a href="#{docs_path}" Docs
				- if admin_signed_in?			
					ul class="nav pull-right"
						li
							div
								= link_to "Logout", :destroy_admin_session, :method => :delete, :class => "btn btn-success btn-large" 


